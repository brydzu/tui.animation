{
    "source": {
        "include": ["src", "README.md"],
        "exclude": [],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
            "outputSourceFiles" : true
        },
        "applicationName": "TOAST UI Animation library",
        "disqus": "",
        "googleAnalytics": "",
        "openGraph": {
            "title": "",
            "type": "website",
            "image": "",
            "site_name": "",
            "url": ""
        },
        "meta": {
            "title": "",
            "description": "",
            "keyword": ""
        },
        "linenums": false
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "name": "animation",
        "recurse": true,
        "destination": "doc",
        "tutorials": "examples",
        "template": "./node_modules/tui-jsdoc-template"
    }
}
